eld><api:field name="authors" type="person-list" display-name="Authors"><api:people><api:person><api:last-name>Patil</api:last-name><api:initials>SR</api:initials><api:first-names>SR</api:first-names><api:separate-first-names><api:first-name>S</api:first-name><api:first-name>R</api:first-name></api:separate-first-names><api:addresses><api:address iso-country-code="IN"><api:line type="organisation">Gulbarga University</api:line><api:line type="city">Gulbarga</api:line><api:line type="country">India</api:line></api:address></api:addresses><api:identifiers><api:identifier scheme="scopus-author-id">57220795101</api:identifier></api:identifiers></api:person><api:person><api:last-name>Ravindra</api:last-name><api:initials></api:initials><api:addresses><api:address iso-country-code="IN"><api:line type="organisation">Gulbarga University</api:line><api:line type="city">Gulbarga</api:line><api:line type="country">India</api:line></api:address></api:addresses><api:identifiers><api:identifier scheme="scopus-author-id">36153419500</api:identifier></api:identifiers></api:person><api:person><api:last-name>Patil</api:last-name><api:initials>SR</api:initials><api:first-names>SR</api:first-names><api:separate-first-names><api:first-name>S</api:first-name><api:first-name>R</api:first-name></api:separate-first-names><api:addresses><api:address iso-country-code="IN"><api:line type="organisation">Gulbarga University</api:line><api:line type="city">Gulbarga</api:line><api:line type="country">India</api:line></api:address></api:addresses><api:identifiers><api:identifier scheme="scopus-author-id">57196715245</api:identifier></api:identifiers></api:person><api:person><api:last-name>Londonkar</api:last-name><api:initials>R</api:initials><api:first-names>R</api:first-names><api:separate-first-names><api:first-name>R</api:first-name></api:separate-first-names><api:addresses><api:address iso-country-code="IN"><api:line type="organisation">Gulbarga University</api:line><api:line type="city">Gulba