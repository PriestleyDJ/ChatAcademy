:keyword scheme="mesh">Dose-Response Relationship, Drug</api:keyword><api:keyword scheme="mesh">Drug Administration Schedule</api:keyword><api:keyword scheme="mesh">Hemorrhage</api:keyword><api:keyword scheme="mesh">Humans</api:keyword><api:keyword scheme="mesh">International Normalized Ratio</api:keyword><api:keyword scheme="mesh">Prospective Studies</api:keyword><api:keyword scheme="mesh">Risk Factors</api:keyword><api:keyword scheme="mesh">Thromboembolism</api:keyword><api:keyword scheme="mesh">Warfarin</api:keyword></api:keywords></api:field><api:field name="language" type="text" display-name="Language"><api:text>eng</api:text></api:field><api:field name="location" type="text" display-name="Country"><api:text>England</api:text></api:field><api:field name="pagination" type="pagination" display-name="Pagination"><api:pagination><api:begin-page>31</api:begin-page><api:end-page>34</api:end-page></api:pagination></api:field><api:field name="publication-date" type="date" display-name="Publication date"><api:date><api:month>1</api:month><api:year>2000</api:year></api:date></api:field><api:field name="publication-status" type="text" display-name="Status"><api:text>Published</api:text></api:field><api:field name="record-made-public-at-source-date" type="date" display-name="Record made publicly available"><api:date><api:day>17</api:day><api:month>3</api:month><api:year>2000</api:year></api:date></api:field><api:field name="title" type="text" display-name="Title"><api:text>A comparison of a low-dose warfarin induction regimen with the modified Fennerty regimen in elderly inpatients.</api:text></api:field><api:field name="types" type="list" display-name="Sub types"><api:items><api:item>Clinical Trial</api:item><api:item>Comparative Study</api:item><api:item>Journal Article</api:item><api:item>Randomized Controlled Trial</api:item></api:items></api:field><api:field name="volume" type="text" display-name="Volume"><api:text>29</api:text></api:field></api:native></api:record><a