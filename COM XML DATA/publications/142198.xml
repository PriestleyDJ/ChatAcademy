.</api:text></api:field><api:field name="authors" type="person-list" display-name="Authors"><api:people><api:person><api:links><api:link type="elements/user" id="1143" href="https://mypublications.shef.ac.uk:8091/secure-api/v6.13/users/1143"/></api:links><api:last-name>Maynard</api:last-name><api:initials>D</api:initials><api:first-names>D</api:first-names><api:separate-first-names><api:first-name>D</api:first-name></api:separate-first-names></api:person><api:person><api:links><api:link type="elements/user" id="70" href="https://mypublications.shef.ac.uk:8091/secure-api/v6.13/users/70"/></api:links><api:last-name>Tablan</api:last-name><api:initials>V</api:initials><api:first-names>V</api:first-names><api:separate-first-names><api:first-name>V</api:first-name></api:separate-first-names></api:person><api:person><api:links><api:link type="elements/user" id="1851" href="https://mypublications.shef.ac.uk:8091/secure-api/v6.13/users/1851"/></api:links><api:last-name>Cunningham</api:last-name><api:initials>H</api:initials><api:first-names>H</api:first-names><api:separate-first-names><api:first-name>H</api:first-name></api:separate-first-names></api:person><api:person><api:last-name>Ursu</api:last-name><api:initials>C</api:initials><api:first-names>C</api:first-names><api:separate-first-names><api:first-name>C</api:first-name></api:separate-first-names></api:person><api:person><api:last-name>Saggion</api:last-name><api:initials>H</api:initials><api:first-names>H</api:first-names><api:separate-first-names><api:first-name>H</api:first-name></api:separate-first-names></api:person><api:person><api:links><api:link type="elements/user" id="1111" href="https://mypublications.shef.ac.uk:8091/secure-api/v6.13/users/1111"/></api:links><api:last-name>Bontcheva</api:last-name><api:initials>K</api:initials><api:first-names>K</api:first-names><api:separate-first-names><api:first-name>K</api:first-name></api:separate-first-names></api:person><api:person><api:last-name>Wilks</api:last-name