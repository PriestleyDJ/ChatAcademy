ion (C) 2010 Elsevier Ltd All rights reserved</api:text></api:field><api:field name="authors" type="person-list" display-name="Authors"><api:people><api:person><api:last-name>Kirchner</api:last-name><api:initials>R</api:initials><api:first-names>R</api:first-names><api:separate-first-names><api:first-name>R</api:first-name></api:separate-first-names></api:person><api:person><api:links><api:link type="elements/user" id="1173" href="https://mypublications.shef.ac.uk:8091/secure-api/v6.13/users/1173"/></api:links><api:last-name>Moore</api:last-name><api:initials>RK</api:initials><api:first-names>RK</api:first-names><api:separate-first-names><api:first-name>R</api:first-name><api:first-name>K</api:first-name></api:separate-first-names></api:person><api:person><api:last-name>Chen</api:last-name><api:initials>TY</api:initials><api:first-names>TY</api:first-names><api:separate-first-names><api:first-name>T</api:first-name><api:first-name>Y</api:first-name></api:separate-first-names></api:person></api:people></api:field><api:field name="doi" type="text" display-name="DOI"><api:text>10.1016/j.wocn.2010.07.005</api:text><api:links><api:link type="doi" href="http://doi.org/10.1016/j.wocn.2010.07.005"/><api:link type="altmetric" href="http://www.altmetric.com/details.php?doi=10.1016/j.wocn.2010.07.005"/></api:links></api:field><api:field name="issn" type="text" display-name="ISSN"><api:text>0095-4470</api:text><api:links><api:link type="elements/journal" href="https://mypublications.shef.ac.uk:8091/secure-api/v6.13/journals/970157"/></api:links></api:field><api:field name="issue" type="text" display-name="Issue"><api:text>4</api:text></api:field><api:field name="journal" type="text" display-name="Journal"><api:text>Journal of Phonetics</api:text></api:field><api:field name="keywords" type="keyword-list" display-name="Keywords"><api:keywords><api:keyword>Linguistics</api:keyword><api:keyword>Memory</api:keyword><api:keyword>Words</api:keyword></api:keywords></api:field><api:fiel