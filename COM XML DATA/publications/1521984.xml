ecification.</api:text></api:field><api:field name="authors" type="person-list" display-name="Authors"><api:people><api:person><api:last-name>Clark</api:last-name><api:initials>AG</api:initials><api:first-names>AG</api:first-names><api:separate-first-names><api:first-name>A</api:first-name><api:first-name>G</api:first-name></api:separate-first-names><api:addresses><api:address iso-country-code="GB"><api:line type="organisation">The University of Sheffield</api:line><api:line type="city">Sheffield</api:line><api:line type="country">United Kingdom</api:line></api:address></api:addresses><api:identifiers><api:identifier scheme="scopus-author-id">57199424465</api:identifier></api:identifiers></api:person><api:person><api:links><api:link type="elements/user" id="31555" href="https://mypublications.shef.ac.uk:8091/secure-api/v6.13/users/31555"/></api:links><api:last-name>Foster</api:last-name><api:initials>M</api:initials><api:first-names>M</api:first-names><api:separate-first-names><api:first-name>M</api:first-name></api:separate-first-names><api:addresses><api:address iso-country-code="GB"><api:line type="organisation">The University of Sheffield</api:line><api:line type="city">Sheffield</api:line><api:line type="country">United Kingdom</api:line></api:address></api:addresses><api:identifiers><api:identifier scheme="scopus-author-id">57204735268</api:identifier></api:identifiers></api:person><api:person><api:links><api:link type="elements/user" id="3470" href="https://mypublications.shef.ac.uk:8091/secure-api/v6.13/users/3470"/></api:links><api:last-name>Walkinshaw</api:last-name><api:initials>N</api:initials><api:first-names>N</api:first-names><api:separate-first-names><api:first-name>N</api:first-name></api:separate-first-names><api:addresses><api:address iso-country-code="GB"><api:line type="organisation">The University of Sheffield</api:line><api:line type="city">Sheffield</api:line><api:line type="country">United Kingdom</api:line></api:address></api:addresses><api